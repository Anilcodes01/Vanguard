generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profiles {
  id         String @id @db.Uuid
  name       String?
  username   String? @unique
  avatar_url String?

  domain       String?
  college_name String?
  onboarded    Boolean   @default(false)

 @@map("profiles") 
}

