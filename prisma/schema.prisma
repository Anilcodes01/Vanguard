enum aal_level {
  aal1
  aal2
  aal3

  @@schema("auth")
}

enum code_challenge_method {
  s256
  plain

  @@schema("auth")
}

enum factor_status {
  unverified
  verified

  @@schema("auth")
}

enum factor_type {
  totp
  webauthn
  phone

  @@schema("auth")
}

enum oauth_registration_type {
  dynamic
  manual

  @@schema("auth")
}

enum one_time_token_type {
  confirmation_token
  reauthentication_token
  recovery_token
  email_change_token_new
  email_change_token_current
  phone_change_token

  @@schema("auth")
}

model Profiles {
  id                  String   @id @db.Uuid
  name                String?
  username            String?  @unique
  avatar_url          String?
  onboarded           Boolean  @default(false)
  xp                  Int      @default(0)
  stars               Int      @default(0)
  domain              String?
  college_name        String?
  year_of_study       String?
  primary_field       String?
  comfort_level       String?
  preferred_langs     String[]
  platform_exp        String?
  main_goal           String[]
  challenge_pref      String[]
  motivation          String[]
  time_dedication     String?
  internship_interest String?
  role_interest       String[]
  project_pref        String?
  playstyle           String?
  first_badge         String?

  league         League            @default(Bronze)
  currentGroupId String?           @map("current_group_id") @db.Uuid
  currentGroup   LeaderboardGroup? @relation(fields: [currentGroupId], references: [id], onDelete: SetNull)
  users          users             @relation(fields: [id], references: [id], onDelete: Cascade)

  @@map("profiles")
  @@schema("public")
}

model LeaderboardGroup {
  id            String     @id @default(uuid()) @db.Uuid
  league        League
  weekStartDate DateTime   @map("week_start_date")
  createdAt     DateTime   @default(now())
  members       Profiles[]

  @@map("leaderboard_groups")
  @@schema("public")
}

enum Difficulty {
  Beginner
  Intermediate
  Advanced

  @@schema("public")
}

model Problem {
  id                     String                   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title                  String
  description            String
  difficulty             Difficulty
  minTime                Int                      @map("min_time")
  maxTime                Int                      @map("max_time")
  slug                   String?                  @unique
  testStrategy           TestStrategy             @map("test_strategy")
  topic                  String[]
  problemLanguageDetails ProblemLanguageDetails[]

  examples    Example[]
  testCases   TestCase[]
  submissions Submission[]
  solutions   ProblemSolution[]

  @@map("problems")
  @@schema("public")
}

model ProblemLanguageDetails {
  id        String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  problemId String @map("problem_id") @db.Uuid

  language           String
  languageId         Int     @map("language_id")
  starterCode        String  @map("starter_code")
  driverCodeTemplate String? @map("driver_code_template")

  problem Problem @relation(fields: [problemId], references: [id], onDelete: Cascade)

  @@unique([problemId, languageId])
  @@map("problem_language_details")
  @@schema("public")
}

model Example {
  id        Int     @id @default(autoincrement())
  input     String?
  output    String?
  problem   Problem @relation(fields: [problemId], references: [id])
  problemId String  @db.Uuid

  @@map("examples")
  @@schema("public")
}

model TestCase {
  id          Int     @id @default(autoincrement())
  input       String?
  expected    String?
  description String?
  problem     Problem @relation(fields: [problemId], references: [id])
  problemId   String  @db.Uuid

  @@map("test_cases")
  @@schema("public")
}

enum TestStrategy {
  STDIN
  DRIVER_CODE

  @@schema("public")
}

enum SubmissionStatus {
  Accepted
  WrongAnswer
  TimeLimitExceeded
  CompilationError
  RuntimeError
  InternalError

  @@map("submission_status")
  @@schema("public")
}

enum SolutionStatus {
  Solved
  Attempted

  @@map("solution_status")
  @@schema("public")
}

model Submission {
  id              String           @id @default(uuid()) @db.Uuid
  userId          String           @map("user_id") @db.Uuid
  problemId       String           @map("problem_id") @db.Uuid
  code            String           @db.Text
  languageId      Int              @map("language_id")
  status          SubmissionStatus
  executionTime   Float?           @map("execution_time")
  executionMemory Int?             @map("execution_memory")
  createdAt       DateTime         @default(now()) @map("created_at")

  user            users            @relation(fields: [userId], references: [id], onDelete: Cascade, map: "submissions_user_id_fkey")
  problem         Problem          @relation(fields: [problemId], references: [id], onDelete: Cascade)
  problemSolution ProblemSolution?

  @@map("submissions")
  @@schema("public")
}

model ProblemSolution {
  userId           String         @map("user_id") @db.Uuid
  problemId        String         @map("problem_id") @db.Uuid
  status           SolutionStatus
  firstSolvedAt    DateTime?      @map("first_solved_at")
  lastAttemptedAt  DateTime       @map("last_attempted_at")
  bestSubmissionId String?        @unique @map("best_submission_id") @db.Uuid
  firstAttemptedAt DateTime?      @map("first_attempted_at")
  starsEarned      Int            @default(0) @map("stars_earned")
  xpEarned         Int            @default(0) @map("xp_earned")

  user           users       @relation(fields: [userId], references: [id], onDelete: Cascade, map: "problem_solutions_user_id_fkey")
  problem        Problem     @relation(fields: [problemId], references: [id], onDelete: Cascade)
  bestSubmission Submission? @relation(fields: [bestSubmissionId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@id([userId, problemId])
  @@map("problem_solutions")
  @@schema("public")
}

model Project {
  id          String  @id @default(cuid())
  name        String  @unique
  description String  @db.Text
  domain      String
  maxTime     String
  coverImage  String?

  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt
  SubmittedProjects SubmittedProjects[]
  ProjectProgress   ProjectProgress[]
  discussions       Discussions[]

@@index([domain]) 
  @@map("projects")
  @@schema("public")
}

enum League {
  Bronze
  Silver
  Gold
  Sapphire
  Ruby
  Emerald
  Amethyst
  Pearl
  Obsidian
  Diamond

  @@schema("public")
}

model SubmittedProjects {
  id                String   @id @default(uuid())
  name              String
  short_description String?
  description       String?
  builtWith         String[]
  githubUrl         String?
  liveUrl           String?
  userId            String
  user              users    @relation(fields: [userId], references: [id], onDelete: Cascade)
  projectId         String
  project           Project  @relation(fields: [projectId], references: [id], onDelete: Cascade)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt

  coverImage  String?
  screenshots String[]

  upvotes   Upvote[]
  comments  Comment[]
  bookmarks Bookmark[]

  @@schema("public")
}

model Upvote {
  userId             String
  submittedProjectId String

  user             users             @relation(fields: [userId], references: [id], onDelete: Cascade)
  submittedProject SubmittedProjects @relation(fields: [submittedProjectId], references: [id], onDelete: Cascade)

  @@id([userId, submittedProjectId])
  @@schema("public")
}

model Comment {
  id                 String   @id @default(uuid())
  text               String
  createdAt          DateTime @default(now())
  userId             String
  submittedProjectId String

  parentId String?
  parent   Comment?  @relation("CommentReplies", fields: [parentId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  replies  Comment[] @relation("CommentReplies")

  user             users             @relation(fields: [userId], references: [id], onDelete: Cascade)
  submittedProject SubmittedProjects @relation(fields: [submittedProjectId], references: [id], onDelete: Cascade)

  likes CommentLike[]

  @@schema("public")
}

model ProjectProgress {
  id        String   @id @default(cuid())
  userId    String   @map("user_id") @db.Uuid
  projectId String   @map("project_id")
  startedAt DateTime @default(now()) @map("started_at")

  user    users   @relation(fields: [userId], references: [id], onDelete: Cascade)
  project Project @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@unique([userId, projectId])
  @@map("project_progress")
  @@schema("public")
}

model Bookmark {
  userId             String @db.Uuid
  submittedProjectId String @db.Uuid

  user             users             @relation(fields: [userId], references: [id], onDelete: Cascade)
  submittedProject SubmittedProjects @relation(fields: [submittedProjectId], references: [id], onDelete: Cascade)

  @@id([userId, submittedProjectId])
  @@schema("public")
}

model Discussions {
  id        String  @id @default(uuid())
  userId    String
  user      users   @relation(fields: [userId], references: [id], onDelete: Cascade)
  project   Project @relation(fields: [projectId], references: [id], onDelete: Cascade)
  projectId String

  @@schema("public")
}

model CommentLike {
  userId    String
  commentId String

  user    users   @relation(fields: [userId], references: [id], onDelete: Cascade)
  comment Comment @relation(fields: [commentId], references: [id], onDelete: Cascade)

  @@id([userId, commentId])
  @@schema("public")
}
